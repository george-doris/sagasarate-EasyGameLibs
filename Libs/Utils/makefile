OBJ_CPP_DEBUG = $(SRC_CPP_FILES:%.cpp=$(OBJDIR_DEBUG)/%.o)

OBJ_C_RELEASE = $(SRC_C_FILES:%.c=$(OBJDIR_RELEASE)/%.o)
OBJ_CPP_RELEASE = $(SRC_CPP_FILES:%.cpp=$(OBJDIR_RELEASE)/%.o)

