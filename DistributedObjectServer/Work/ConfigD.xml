    </Lib>
    <Lib Name="Protocol" NeedCompile="false" OutDir="lib">
      <SourceDir Name="protocol"/>
    </Lib>
  </Libs>
  
  <Plugins>
    <PluginObjectPool StartSize="4096" GrowSize="1024" GrowLimit="32" />
    <Plugin  Type="CSharp" Name="CSTest" LoadType="Assembly" MainClassNameSpace="CSTest" MainClass="CSTestMain" >
      <SourceDir Name="CSTest"/>
    </Plugin>
  </Plugins>

  <System>
